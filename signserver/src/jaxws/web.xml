<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">
  <display-name>htmfadminws</display-name>
  <description>htmfadminws</description>
  <listener>
    <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>htmfadminws</servlet-name>
    <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>htmfadminws</servlet-name>
    <url-pattern>/htmfadminws</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <env-entry>
    <env-entry-name>DataSource</env-entry-name>
    <env-entry-value>java:/EjbcaDS</env-entry-value>
    <env-entry-type>java.lang.String</env-entry-type>
  </env-entry>
