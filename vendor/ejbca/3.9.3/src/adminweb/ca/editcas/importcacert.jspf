<div align="center">   
	<h2><%= ejbcawebbean.getText("IMPORTCACERT") %><br></h2>
</div>
	<table width="100%" border="0" cellspacing="3" cellpadding="3">
		<tr id="Row<%=row++%2%>"> 
			<td width="49%" valign="top"> </td>
			<td width="51%" valign="top"> 
        		<div align="right">
        			<A href="<%=THIS_FILENAME %>"><u><%= ejbcawebbean.getText("BACKTOCAS") %></u></A>
				</div>
			</td>
		</tr>
		<form name="recievefile" action="<%= THIS_FILENAME %>" method="post" enctype='multipart/form-data' >
		<tr id="Row<%=row++%2%>"> 
			<td width="49%" valign="top" align="right"><%= ejbcawebbean.getText("IMPORTCANAMEHELP") %></td>
			<td width="51%" valign="top">
					<input type="text" name='<%= TEXTFIELD_IMPORTCA_NAME %>' value=''>
			</td>
		</tr>
		<tr id="Row<%=row++%2%>"> 
			<td width="49%" valign="top" align="right"><%= ejbcawebbean.getText("IMPORTCACERTFILEHELP") %></td>
			<td width="51%" valign="top">
					<input type="hidden" name='<%= ACTION %>' value='<%=ACTION_IMPORTCACERT %>'>
					<input TYPE="FILE" NAME="<%= FILE_RECIEVEFILE %>">
			</td>
		</tr>
		<tr id="Row<%=row++%2%>"> 
			<td width="49%" valign="top" align="right"> </td>
			<td width="51%" valign="top">
					<input type="submit" name="<%= BUTTON_RECIEVEFILE %>" onClick='return check()' value="<%= ejbcawebbean.getText("IMPORTCACERT") %>" >
					<input type="submit" name="<%= BUTTON_CANCEL %>" value="<%= ejbcawebbean.getText("CANCEL") %>">     
			</td>
		</tr>
		</form>
	</table>
